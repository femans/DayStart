{
  "name": "nuxt-app",
  "version": "0.0.1",
  "description": "Project management for dummies",
  "author": "Frido Emans",
  "homepage": "https://daystart.io",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "supa-types": "pnpx supabase gen types --lang=typescript --project-id \"$PROJECT_REF\" > types/database.types.ts",
    "supa-types-local": "pnpx supabase gen types typescript --local > types/database.types.ts"
  },
  "COMMENT_ON_SUPABASE_REALTIME": "The version of @supabase/realtime-js is pinned to 2.10.9 to be compatible with @nuxtjs/supabase. So this must be updated alongside with that.",
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@nuxtjs/supabase": "^1.4.6",
    "@supabase/realtime-js": "2.11.2",
    "nuxt": "^3.15.3",
    "vue": "latest",
    "vue-arrange": "^1.3.3",
    "vue-router": "latest",
    "yup": "^1.6.1"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b",
  "devDependencies": {
    "@nuxt/eslint": "^0.6.2",
    "@nuxt/icon": "^1.10.3",
    "@nuxt/ui": "^2.21.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "eslint": "^9.19.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.2",
    "typescript": "^5.7.3"
  },
  "resolutions": {
    "cross-spawn": "^7.0.5"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "module": "esnext",
  "peerDependencies": {
    "@supabase/realtime-js": "2.10.9"
  }
}
